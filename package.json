    "prepublish:demo": "npm run build:demo",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.1",
    "gh-pages": "^5.0.0",
    "rollup": "^3.20.7",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.0"
  }
}
